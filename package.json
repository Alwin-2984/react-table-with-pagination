{
  "name": "@alwinkc/tanstack-table-components",
  "version": "1.0.7",
  "description": "A React table component library built on TanStack Table with pagination support",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./styles.css": "./dist/styles.css"
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "npm run build:js && npm run build:css",
    "build:js": "vite build && tsc --emitDeclarationOnly --declaration --declarationMap",
    "build:css": "npx tailwindcss -i ./src/styles.css -o ./dist/styles.css --minify",
    "dev": "vite build --watch",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "react",
    "table",
    "tanstack",
    "pagination",
    "component",
    "ui"
  ],
  "author": "Alwin KC",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Alwin-2984/react-table-with-pagination.git"
  },
  "bugs": {
    "url": "https://github.com/Alwin-2984/react-table-with-pagination/issues"
  },
  "homepage": "https://github.com/Alwin-2984/react-table-with-pagination#readme",
  "peerDependencies": {
    "@tanstack/react-table": "^8.0.0",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "dependencies": {
    "clsx": "^2.1.0"
  },
  "devDependencies": {
    "@tanstack/react-table": "^8.11.6",
    "@types/node": "^20.10.6",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "autoprefixer": "^10.4.23",
    "postcss": "^8.5.6",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "tailwindcss": "^3.4.19",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vite-plugin-dts": "^3.7.0"
  }
}
